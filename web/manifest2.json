{
    "name": "Fleeting Notes",
    "description": "A note-taking app for capturing and connecting fleeting moments.",
    "version": "0.1.5",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "/icons/196.png"
    },
    "permissions": [
      "activeTab"
    ],
    "commands": {
      "_execute_browser_action": {
        "suggested_key" : {
         "default": "Ctrl+Shift+E",
         "mac": "Command+Shift+E"
        }
      }
    },
    "manifest_version": 2
  }
  